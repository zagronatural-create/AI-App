# Staging environment template for Supply Intelligence API
# Copy to staging.env (or your secret manager) and replace placeholders.

DATABASE_URL=postgresql+psycopg://postgres:postgres@staging-postgres:5432/supply_intel
APP_ENV=staging
STORAGE_DIR=storage
AUTH_ENABLED=true
API_TOKEN_MAP_JSON={"replace-admin-token":{"user_id":"admin.staging","roles":["admin"]},"replace-qa-token":{"user_id":"qa.staging","roles":["qa_manager","qa_analyst"]},"replace-ops-token":{"user_id":"ops.staging","roles":["ops_scheduler"]},"replace-viewer-token":{"user_id":"auditor.staging","roles":["viewer"]}}
CORS_ALLOW_ORIGINS=https://ops-staging.example.com
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS=300
RATE_LIMIT_WINDOW_SECONDS=60
